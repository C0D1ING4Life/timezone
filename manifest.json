{
    "manifest_version":2,
    "name":"Timezone/ App Finder",
    "version":"2.0.1",
    "description":"This is a sample extension that displays your current timezone",
    "icons":{
        "128":"clock.png"
    },
    "browser_action":{
        "default_popup":"popup.html",
        "defalt_icon":"clock.png",
        "default_title":"Timezone/ App Finder"
    },
"permissions":["activeTab"],
"background":{
    "scripts":["main.js"]
  }
}
